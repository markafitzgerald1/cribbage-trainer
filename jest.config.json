{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.{ts,tsx}", "!src/**/*.d.ts"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "moduleNameMapper": {
    "\\.css$": "identity-obj-proxy"
  },
  "testEnvironment": "jsdom",
  "testPathIgnorePatterns": ["/tests-e2e/", "/tests-examples"]
}
