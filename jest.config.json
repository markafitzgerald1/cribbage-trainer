{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.{ts,tsx}"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "testEnvironment": "jsdom",
  "testPathIgnorePatterns": ["/tests-e2e/", "/tests-examples"]
}
