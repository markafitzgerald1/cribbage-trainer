{
  "name": "cribbage-trainer",
  "description": "Two-player cribbage discard and play improvement tool.",
  "version": "0.1.0",
  "source": "src/index.html",
  "scripts": {
    "prepare": "husky install",
    "test": "jest --verbose",
    "test-e2e": "playwright test",
    "lintThenTypeCopyPasteOutdatedAndAuditCheck": "npm run lint && npm run typeCheck && npm run jscpd && npm outdated && npm audit",
    "lint": "eslint --max-warnings 0 . && stylelint '**/*.css' && markdownlint README.md && prettier --check . && cspell '**' --gitignore",
    "stylelint": "stylelint --fix '**/*.css'",
    "prettier": "prettier --write .",
    "typeCheck": "tsc",
    "jscpd": "jscpd",
    "build": "parcel build --reporter @parcel/reporter-bundle-analyzer --public-url /cribbage-trainer --detailed-report 49",
    "start": "parcel serve --reporter @parcel/reporter-bundle-analyzer --public-url /cribbage-trainer",
    "clean": "rm -rf dist parcel-bundle-reports coverage .parcel-cache"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@parcel/packager-raw-url": "^2.10.3",
    "@parcel/reporter-bundle-analyzer": "^2.10.3",
    "@parcel/transformer-css": "^2.10.3",
    "@parcel/transformer-webmanifest": "^2.10.3",
    "@playwright/test": "^1.40.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "babel-jest": "^29.7.0",
    "cspell": "^8.1.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-spellcheck": "^0.0.20",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jscpd": "^3.5.10",
    "markdownlint-cli": "^0.37.0",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0",
    "process": "^0.11.10",
    "seedrandom": "^3.0.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markafitzgerald1/cribbage-trainer.git"
  },
  "keywords": [
    "cribbage",
    "trainer",
    "analysis"
  ],
  "author": "Mark A. Fitzgerald <mark.a.fitzgerald1@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/markafitzgerald1/cribbage-trainer/issues"
  },
  "homepage": "https://github.com/markafitzgerald1/cribbage-trainer#readme",
  "dependencies": {
    "js-combinatorics": "^2.1.2",
    "Package": "^0.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "dashedIdents": true
    }
  }
}
